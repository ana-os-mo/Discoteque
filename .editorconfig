root=true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
max_line_length = 150

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[*.html]
max_line_length = off

[*.{sln,sln.DotSettings}]
indent_style = tab
insert_final_newline = unset
